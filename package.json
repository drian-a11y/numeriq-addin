{
  "name": "numeriq-excel-addin",
  "version": "1.0.0",
  "description": "Advanced Excel formula exploration and analysis tool",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "dev": "webpack serve --mode development --open",
    "build:dev": "webpack --mode development",
    "clean": "rimraf dist",
    "validate": "tsc --noEmit",
    "lint": "echo 'Linting not configured yet'",
    "prebuild": "npm run clean",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "keywords": [
    "excel",
    "office-addin",
    "formula-explorer",
    "spreadsheet-analysis"
  ],
  "author": "Numeriq",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/drian-a11y/numeriq-addin"
  },
  "dependencies": {
    "@microsoft/office-js": "^1.1.91",
    "@microsoft/office-js-helpers": "^1.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.0",
    "@types/office-js": "^1.0.376",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "gh-pages": "^6.3.0",
    "html-webpack-plugin": "^5.5.3",
    "office-addin-dev-certs": "^2.0.3",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.3",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  }
